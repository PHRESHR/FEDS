{
  "node": true,
  "browser": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": false,
  "curly": true,
  "eqeqeq": true,
  "expr": true,
  "immed": true,
  "indent": 2,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "undef": true,
  "unused": "vars",
  "strict": false,
  "globals": {
    "_": false,
    "wrap": true,
    "unwrap": true
  },
  "predef": [
    "System",
    "angular",
    "_gaq",
    "module",
    "exports",
    "define",
    "Modernizr",
    "jasmine",
    "protractor",
    "phantom"
  ]
}
